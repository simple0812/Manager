{
  "dir": "E:\\projects\\manager\\Manager",
  "dll": {
    "name": "vendor",
    "target": "var",
    "content": [
      "babel-polyfill",
      "react",
      "react-dom",
      "history",
      "react-router",
      "react-router-dom",
      "redux",
      "react-redux",
      "redux-saga",
      "redux-actions",
      "axios"
    ]
  },
  "directory": {
    "development": {
      "envName": "src",
      "component": "components",
      "container": "containers",
      "redux": "redux",
      "store": "store",
      "util": "utils",
      "config": "configs",
      "sprite": "sprite",
      "style": "less",
      "data": "json",
      "test": "__test__",
      "document": "doc",
      "source": "src"
    },
    "build": {
      "envName": ".build"
    },
    "production": {
      "envName": "dist",
      "resource": "resources",
      "dll": "dll",
      "javascript": "js",
      "style": "css",
      "asset": "images"
    }
  },
  "general": {
    "publicPath": "",
    "resolve": {
      "alias": {
        "rework": "rework.less/rework.less",
        "sprite": "sprite.less"
      },
      "extensions": [
        ".web.js",
        ".js",
        ".json"
      ],
      "mainFields": [
        "browser",
        "jsnext:main",
        "main"
      ]
    },
    "externals": {
      "configs": "configs"
    },
    "performance": {
      "maxAssetSize": 400000,
      "maxEntrypointSize": 600000
    },
    "target": "web"
  },
  "compiler": {
    "css": {
      "less": {
        "minimize": true
      },
      "postcss": {
        "autoprefixer": {
          "browsers": [
            "last 3 versions",
            "> 1%"
          ]
        },
        "pxtorem": {
          "rootValue": "100",
          "propWhiteList": []
        }
      }
    },
    "asset": {
      "image": {
        "optimizer": {
          "limit": 10000
        }
      },
      "svg": {
        "sprite": {
          "extract": false
        }
      }
    },
    "html": {
      "title": "Single Page Application",
      "description": "description",
      "keywords": "keywords"
    }
  },
  "theme": {
    "border-radius-base": "0px",
    "border-radius-sm": "0px",
    "primary-color": "#0b87fd",
    "brand-primary": "#43a5df",
    "brand-primary-tap": "#43a5df"
  },
  "debug": {
    "port": 3000,
    "sync": false,
    "browsersync": {
      "port": 4000,
      "open": true
    }
  },
  "preview": {
    "port": 3000
  },
  "deploy": {
    "host": "",
    "port": "",
    "token": "",
    "environment": "Node"
  },
  "id": "e13e68c0-d408-11e7-af7f-ff675c245fc5",
  "type": "pcWeb",
  "name": "Manager",
  "productName": "福禄管家",
  "description": "福禄管家",
  "version": "1.0.0",
  "compatibility": {
    "chrome": "latest",
    "firefox": "latest",
    "safari": "latest",
    "ie": "9+"
  },
  "routerType": "BrowserRouter"
}