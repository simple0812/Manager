{
  "name": "Bundle",
  "description": "代码拆分（Code Splitting）组件，需配合 bundle-loader 一起使用",
  "version": "1.0.0",
  "compatibility": {
    "chrome": "latest",
    "firefox": "latest",
    "safari": "latest",
    "ie": "9+"
  },
  "module": [
    "doc"
  ],
  "format": "class",
  "lifecycleMountingOptions": [
    "constructor",
    "render",
    "componentWillMount"
  ],
  "lifecycleUpdatingOptions": [
    "componentWillReceiveProps"
  ],
  "isPropCheck": true,
  "note": "",
  "type": 0,
  "lifecycle": [
    "constructor",
    "render",
    "componentWillMount",
    "componentWillReceiveProps"
  ],
  "author": "尹传武",
  "camelCaseName": "bundle"
}